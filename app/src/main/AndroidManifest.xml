<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
          package="com.moneymanager">

    <application
        android:name="android.support.multidex.MultiDexApplication"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/Theme.AppCompat.NoActionBarAppTheme">
        <activity android:name=".activities.AMain">
            <intent-filter>
                <action android:name="android.intent.action.MAIN"/>

                <category android:name="android.intent.category.LAUNCHER"/>
            </intent-filter>
        </activity>
        <activity android:name=".activities.accounts.AAccounts">
        </activity>
        <activity android:name=".activities.search.ASearch">
        </activity>
        <activity android:name=".activities.transaction.AAddTransaction">
        </activity>
        <activity android:name=".activities.accounts.AAddAccount">
        </activity>
        <activity android:name=".activities.category.ACategories">
        </activity>
        <activity
            android:name=".activities.stats.AStats"
            android:theme="@style/Theme.AppCompat.NoActionBarAppTheme.Darker">
        </activity>
        <activity android:name=".activities.transaction.AEditTransaction">
        </activity>
        <activity android:name=".activities.AUser">
        </activity>
        <activity android:name=".activities.debts.ADebts">
        </activity>
        <activity android:name=".activities.accounts.AAccountTransfer">
        </activity>
        <activity android:name=".activities.budget.AAddBudget">
        </activity>
        <activity android:name=".activities.budget.ABudgets">
        </activity>
        <activity android:name=".activities.debts.AEditDebt">
        </activity>
        <activity android:name=".activities.ASettings">
        </activity>
        <activity android:name=".activities.search.ASearchResults">
        </activity>
        <activity android:name=".activities.accounts.ALedger">
        </activity>
    </application>

</manifest>